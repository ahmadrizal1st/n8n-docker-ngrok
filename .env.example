# n8n with Ngrok Environment Template
# Copy this file to .env and update the values for your environment

# Ngrok Configuration
NGROK_AUTH_TOKEN=your_ngrok_authtoken_from_dashboard
NGROK_REGION=us
NGROK_DOMAIN=your-static-domain.ngrok.io

# n8n Basic Configuration
N8N_PORT=5678
NODE_ENV=production
GENERIC_TIMEZONE=Asia/Jakarta
N8N_HOST=0.0.0.0

# n8n Security Configuration (Generate secure random values)
N8N_ENCRYPTION_KEY=change_this_to_a_secure_random_string_min_32_chars
N8N_USER_MANAGEMENT_JWT_SECRET=change_this_to_another_secure_random_string

# Database Configuration (Optional)
# N8N_DATABASE_TYPE=postgresdb
# N8N_DB_POSTGRESDB_DATABASE=n8n
# N8N_DB_POSTGRESDB_HOST=postgres
# N8N_DB_POSTGRESDB_PORT=5432
# N8N_DB_POSTGRESDB_USER=username
# N8N_DB_POSTGRESDB_PASSWORD=password

# External URL Configuration
# WEBHOOK_URL will be auto-configured by startup script